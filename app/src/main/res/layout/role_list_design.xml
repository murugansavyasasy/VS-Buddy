<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/two"
    android:layout_marginTop="@dimen/one"
    android:layout_marginRight="@dimen/two"
    android:background="@color/white"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rytOverAll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/five"
        android:padding="@dimen/fifteen"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imgCollegeLogo"
                android:layout_width="@dimen/eighty"
                android:layout_height="@dimen/eighty"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/twenty"
                android:layout_marginTop="@dimen/eight"
                android:src="@drawable/college_logo" />


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/twenty_five"
                android:layout_toRightOf="@+id/imgCollegeLogo">

                <TextView
                    android:id="@+id/lblMemberName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="@dimen/five"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/twenty"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/lblCollegeName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lblMemberName"
                    android:layout_marginLeft="@dimen/five"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/fifteen" />

                <TextView
                    android:id="@+id/lbl_Deptname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lblCollegeName"
                    android:layout_marginLeft="@dimen/five"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/fifteen" />

                <TextView
                    android:id="@+id/lblCoursenname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lbl_Deptname"
                    android:layout_marginLeft="@dimen/five"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/fourteen" />

                <TextView
                    android:id="@+id/lblYearName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lblCoursenname"
                    android:layout_marginLeft="@dimen/five"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/thirteen"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/viewline1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lblCoursenname"
                    android:layout_marginLeft="@dimen/five"
                    android:layout_toRightOf="@+id/lblYearName"
                    android:text="-"
                    android:textColor="@color/black"
                    android:textSize="@dimen/thirteen"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/semname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lblCoursenname"
                    android:layout_marginLeft="@dimen/five"
                    android:layout_toRightOf="@+id/viewline1"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/thirteen"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/lblSectionname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/semname"
                    android:layout_marginLeft="@dimen/five"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/fifteen" />

            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
